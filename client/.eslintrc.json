{
    "extends": "google",
    "env": {
        "browser": true,
        "mocha": true
    },
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules" : {
        "react/jsx-uses-react": 1
    }
}